			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody);\r",
							"//pm.environment.set(\"access_token\", jsonData.access_token);\r",
							"pm.test(\"Verify geoPointAddress response correct lat/long\", function () {\r",
							"    pm.expect(jsonData.data.geoPointAddress.lat != null);\r",
							"    console.log(jsonData.data.geoPointAddress.lat );\r",
							"    pm.expect(jsonData.data.geoPointAddress.long != null);\r",
							"     console.log(jsonData.data.geoPointAddress.long);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"full_address\": {{full_address_slic}}\r\n   // \"full_address\": \"@@\"\r\n}"
				},
				"url": {
					"raw": "https://stg-slic.eton.vn/api/v1/address/search?_format=json&exact=true",
					"protocol": "https",
					"host": [
						"stg-slic",
						"eton",
						"vn"
					],
					"path": [
						"api",
						"v1",
						"address",
						"search"
					],
					"query": [
						{
							"key": "_format",
							"value": "json"
						},
						{
							"key": "exact",
							"value": "true"
						}
					]
				}
			},
			"response": []
